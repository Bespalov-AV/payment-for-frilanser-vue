{
  "manifest_version": 2,
  "name": "payment-for-frilanser-vue",
  "description": "Пример реализации простого расширения для Chrome для ведения оплат на vue.js",
  "version": "1.0",

  "background": {
    "scripts": [
      "vueclass.js"
    ],
    "persistent": false
  },

  "browser_action": {
    "default_icon": {"32": "bitcoin32.ico"},
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
